<layout-macro-def id="camera-summary-fragment">
  <params>
    <param name="camera" type="Camera" max="One" min="Required"/>
  </params>
  <content>
    <layout-macro id="common:card">
      <title>{{camera.name}}</title>
      <image>{{camera.image.images[0].url}}</image>
      <imageType>background</imageType>
      <aspectRatio>4_3</aspectRatio>
    </layout-macro>
  </content>
  <style></style>
</layout-macro-def>
