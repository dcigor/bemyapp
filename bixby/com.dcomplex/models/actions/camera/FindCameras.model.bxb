action (FindCameras) {
  type (Search)
  collect {
    input (name) {
      type (CameraName)
      min (Optional)
    }
    input (count) {
      type (CameraCount)
      min (Optional)
      default-init {
        intent {
          goal {Camera}
          value {
            CameraCount (10)
          }
        }
      }
    }
  }
  output (Camera)
}
